.project
  %h3= project['title']

  %p= project['description']

  %p
    %small
      %strong Kunde:
      = project['customer']
      &ndash;
      %strong Meine Rolle:
      = project['role']
      &ndash;
      %strong Dauer:
      = project['start']
      - unless '' == project['end']
        bis
        = project['end']
      &ndash;
      %strong Werkzeuge:
      = project['tools']
